<div class="col-lg stageBlock ms-1 me-1">
    <div class="row">
        <div class="col">
            <h2 class="stage">{{ ibexa_content_name(content) }}</h2>
        </div>
    </div>
    <div class="row align-items-center">
        <div class="col">
        {% for match in matches %}
            {{ ibexa_render(match, {'viewType': 'embed', 'method': 'esi'}) }}
        {% endfor %}
        </div>
    </div>
</div>
